{
  "name": "gyfchong-portfolio",
  "description": "Geoffrey Chong's portfolio",
  "author": {
    "name": "Geoffrey Chong",
    "url": "http://geoffreychong.com"
  },
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gyfchong/portfolio"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.4.0",
  "scripts": {
    "start": "concurrently \"pnpm dev\" \"pnpm dev:css\"",
    "dev": "npx @11ty/eleventy --serve",
    "dev:css": "pnpm build:css --watch",
    "build": "rimraf _site && pnpm build:css && npx @11ty/eleventy",
    "build:css": "postcss src/styles/_index.css -o src/_includes/styles.css"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "reset.css": "^2.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-dev-server": "^1.0.4",
    "autoprefixer": "^10.4.14",
    "clean-css": "^5.3.2",
    "concurrently": "^8.0.1",
    "cssnano": "^6.0.1",
    "eleventy-plugin-postcss": "^1.0.4",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "rimraf": "^5.0.0",
    "svgo": "^3.0.2"
  }
}
